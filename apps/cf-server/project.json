{
  "name": "cf-server",
  "sourceRoot": "apps/cf-server/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter cf-server dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter cf-server build"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter cf-server deploy"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter cf-server lint"
      }
    }
  }
}